{
  "name": "radio-rasclat-web",
  "version": "4.3.0",
  "private": false,
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --prod",
    "build:stats": "npm run env -s && ng build --prod --stats-json",
    "start": "npm run env -s && ng serve --aot",
    "serve:sw": "npm run build -s && npx http-server ./dist -p 4200",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test --code-coverage --watch=false",
    "test:ci": "npm run env -s && npm run lint -s && ng test --code-coverage --watch=false",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean --sort --marker marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "prettier": "prettier --write \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
    "prettier:check": "prettier --list-different \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
    "generate": "ng generate",
    "generate-icons": "svg-to-ts",
    "analyze": "webpack-bundle-analyzer dist/stats.json"
  },
  "dependencies": {
    "@angular/animations": "^10.2.5",
    "@angular/common": "^10.2.5",
    "@angular/compiler": "^10.2.5",
    "@angular/core": "^10.2.5",
    "@angular/forms": "^10.2.5",
    "@angular/localize": "^10.2.5",
    "@angular/platform-browser": "^10.2.5",
    "@angular/platform-browser-dynamic": "^10.2.5",
    "@angular/router": "^10.2.5",
    "@angular/service-worker": "^10.2.5",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@ngneat/cashew": "^1.3.2",
    "@ngneat/svg-icon": "^1.0.3",
    "@ngx-translate/core": "^13.0.0",
    "@sentry/browser": "^5.30.0",
    "@sentry/cli": "^1.67.1",
    "@sentry/integrations": "^5.30.0",
    "@types/algoliasearch-helper": "^2.26.1",
    "algoliasearch": "^3.35.1",
    "angular-instantsearch": "^3.0.0-beta.5",
    "angulartics2": "10.0.0",
    "animate.css": "^4.1.1",
    "codecov": "^3.8.2",
    "core-js": "^3.15.2",
    "flashgrid": "^1.1.1",
    "instantsearch.css": "^7.4.5",
    "instantsearch.js": "^3.7.0",
    "klaro": "^0.7.18",
    "medium-zoom": "^1.0.6",
    "moment": "^2.29.1",
    "ng-lazyload-image": "^9.1.0",
    "ng2-tooltip-directive": "^2.9.22",
    "ngx-cookie-service": "^10.1.1",
    "ngx-moment": "^5.0.0",
    "ngx-skeleton-loader": "^2.10.1",
    "ngx-swiper-wrapper": "^9.0.1",
    "ngx-timeago": "^1.0.4",
    "ngx-toastr": "^13.2.1",
    "node-vibrant": "^3.1.6",
    "rxjs": "^6.6.6",
    "rxjs-compat": "^6.6.6",
    "tslib": "^2.3.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.1.2",
    "@angular/cli": "^10.2.2",
    "@angular/compiler-cli": "^10.2.5",
    "@angular/language-service": "^10.2.5",
    "@biesbjerg/ngx-translate-extract": "^7.0.4",
    "@ngx-rocket/scripts": "^4.0.0",
    "@types/algoliasearch": "^3.34.11",
    "@types/jasmine": "^3.8.1",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^14.17.5",
    "codelyzer": "^6.0.2",
    "hads": "^2.1.0",
    "htmlhint": "^0.14.2",
    "https-proxy-agent": "^5.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "karma-junit-reporter": "^2.0.1",
    "prettier": "^2.3.2",
    "protractor": "~7.0.0",
    "puppeteer": "^4.0.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "^4.3.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.19.0",
    "svg-to-ts": "^5.7.1",
    "ts-node": "~8.10.2",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "^3.9.10",
    "webpack-bundle-analyzer": "^4.4.2"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  },
  "svg-to-ts": {
    "conversionType": "object",
    "srcFiles": [
      "./src/assets/svg/*.svg"
    ],
    "outputDirectory": "./src/assets/svg",
    "fileName": "svg-icons",
    "svgoConfig": {
      "plugins": [
        {
          "removeDimensions": true,
          "cleanupAttrs": true
        }
      ]
    }
  }
}
